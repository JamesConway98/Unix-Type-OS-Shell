/*
   Function declarations
*/

void user_prompt(void);




int main(int argc, char const *argv[]) {

if (argc == 2){
	if(!strcmp(argv[1],"-f"))
		readFile();
	else if (!strcmp(argv[1], "-c"))
		user_prompt();
    }
    
    else if (argc > 2)
	printf("Too many arguments supplied.\n");	
    else 
	printf("One argument expected");

    return 0;
}






void user_prompt(){
	char input[512];
	int	online = 1;
While(online == 1)
	Printf("Enter your input here:\n")
	fgets(input);
	switch(input)
		case 'command':
			printf("This Will be a command run by the shell");
			break;
		case 'Exit':
			online = 0;
			break;
		default:
			Printf("This is not a valid command")

}